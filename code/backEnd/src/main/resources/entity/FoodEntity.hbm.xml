<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="lakers.ingram.ModelEntity.FoodEntity" table="food" schema="SummerProj">
        <id name="foodId">
            <column name="foodID" sql-type="int(11)"/>
        </id>
        <property name="foodName">
            <column name="foodName" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="price">
            <column name="price" sql-type="float" precision="-1" not-null="true"/>
        </property>
        <property name="tips">
            <column name="tips" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="windowId">
            <column name="window_id" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="likes">
            <column name="likes" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>