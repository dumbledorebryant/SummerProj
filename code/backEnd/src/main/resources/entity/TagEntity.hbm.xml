<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="lakers.ingram.ModelEntity.TagEntity" table="tag" schema="SummerProj">
        <id name="tagId">
            <column name="tagID" sql-type="int(11)"/>
        </id>
        <property name="tagName">
            <column name="tagName" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="tagType">
            <column name="tagType" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>