<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="lakers.ingram.ModelEntity.AdminEntity" table="admin" schema="summerproj">
        <id name="adminId">
            <column name="adminID" sql-type="int(11)"/>
        </id>
        <property name="password">
            <column name="password" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="phone">
            <column name="phone" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>