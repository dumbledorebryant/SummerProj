<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="lakers.ingram.ModelEntity.TodayfoodEntity" table="todayfood" schema="summerproj">
        <id name="foodId">
            <column name="foodID" sql-type="int(11)"/>
        </id>
        <property name="date">
            <column name="date" sql-type="date"/>
        </property>
        <property name="time">
            <column name="time" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>