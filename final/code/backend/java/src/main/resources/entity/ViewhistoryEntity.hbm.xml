<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="lakers.ingram.ModelEntity.ViewhistoryEntity" table="viewhistory" schema="summerproj">
        <composite-id>
            <key-property name="userId">
                <column name="userId" sql-type="int(11)"/>
            </key-property>
            <key-property name="windowId">
                <column name="windowId" sql-type="int(11)"/>
            </key-property>
        </composite-id>
        <property name="count">
            <column name="count" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>