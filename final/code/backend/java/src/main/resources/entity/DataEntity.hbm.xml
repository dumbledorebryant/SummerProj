<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="lakers.ingram.ModelEntity.DataEntity" table="data" schema="SummerProj">
        <id name="dataId">
            <column name="dataId" sql-type="int(11)"/>
        </id>
        <property name="number">
            <column name="number" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="date">
            <column name="date" sql-type="datetime" not-null="true"/>
        </property>
        <property name="windowId">
            <column name="windowId" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>