<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="lakers.ingram.ModelEntity.FoodtagEntity" table="foodtag" schema="SummerProj">
        <composite-id>
            <key-property name="tagId">
                <column name="tagID" sql-type="int(11)"/>
            </key-property>
            <key-property name="foodId">
                <column name="foodID" sql-type="int(11)"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>